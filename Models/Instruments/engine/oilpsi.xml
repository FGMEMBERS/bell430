<?xml version="1.0"?>

<PropertyList>

<path>oilpsi.ac</path>

 <animation>
  <type>scale</type>
  <x-offset>0.59</x-offset>
  <y-offset>0.59</y-offset>
  <z-offset>0.59</z-offset>
 </animation>

    <animation>
        <type>material</type>
        <object-name>OILpsi.face</object-name>
        <object-name>OILpsi.needle</object-name>
        <emission>
            <red>0.0175</red>
            <green>0.0175</green>
            <blue>0.0175</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>
