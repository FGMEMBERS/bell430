<?xml version="1.0"?>

<PropertyList>

	
 
 
    <path>bigmfd.ac</path>
	
	
 <!-- engine 1 -->
<model>
		<path>Aircraft/bell430/Models/Instruments/bigmfd/IIDS/iids.xml</path>
		<offsets>
			<x-m>0.01</x-m>
			<y-m>0.1</y-m>
			<z-m>-0.01</z-m>
			<pitch-deg>0</pitch-deg>
		</offsets>
		<overlay>
			
			 <animation n="100">
    <type>scale</type>
    <property>constants/zero</property>
    <x-offset>1</x-offset>
    <y-offset>0.63</y-offset>
    <z-offset>0.7</z-offset>
   </animation>
		</overlay>
		
	</model>

	

<!-- Engine 2		 -->
